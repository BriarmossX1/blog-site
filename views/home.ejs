<%- include("partials/header") -%>

    <h1>Home</h1>

    <p>
        <%= startingContent %>
    </p>

    <% blogposts.forEach((homePost) => { %>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                    <%= homePost.Title %>
                </h3>
            </div>
            <div class="panel-body">
                <p>
                    <%= homePost.Content %>
                        <a href="/post/<%= homePost.Title %>">Read More</a>
                </p>
            </div>
        </div>
        <% }) %>

